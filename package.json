{
  "name": "share-timer",
  "version": "0.0.0",
  "author": "Kazuma Ebina <kazuma1989@quipper.com>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazuma1989/share-timer"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.6.0"
  },
  "type": "module",
  "scripts": {
    "start": "vite serve",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write \"./{public,src}/**/*.{css,html,js,ts,tsx}\"",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:unused": "npx -y ts-unused-exports ./tsconfig.json",
    "upgrade": "npx -y npm-check-updates --upgrade",
    "deploy": "firebase deploy",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "firebase-login": "firebase login",
    "firebase-init": "firebase init",
    "firebase-target-apply": "firebase target:apply hosting app sharetimer",
    "firebase-indexes-export": "firebase firestore:indexes > firestore.indexes.json",
    "firebase-emulators-init": "firebase init emulators",
    "firebase-emulators-start": "firebase emulators:start --only firestore"
  },
  "dependencies": {
    "@emotion/css": "11.10.x",
    "@tsconfig/vite-react": "1.0.x",
    "@vitejs/plugin-react": "2.1.x",
    "firebase": "9.12.x",
    "firebase-tools": "11.14.x",
    "react": "18.2.x",
    "react-dom": "18.2.x",
    "vite": "3.1.x",
    "zod": "3.19.x"
  },
  "devDependencies": {
    "@types/node": "18.x.x",
    "@types/react": "18.0.x",
    "@types/react-dom": "18.0.x",
    "@typescript-eslint/eslint-plugin": "5.40.x",
    "@typescript-eslint/parser": "5.40.x",
    "eslint": "8.25.x",
    "eslint-config-prettier": "8.5.x",
    "eslint-plugin-react": "7.31.x",
    "eslint-plugin-react-hooks": "4.6.x",
    "prettier": "2.7.x",
    "prettier-plugin-organize-imports": "3.1.x",
    "typescript": "4.8.x",
    "vite-plugin-checker": "0.5.x",
    "vitest": "0.24.x"
  }
}
